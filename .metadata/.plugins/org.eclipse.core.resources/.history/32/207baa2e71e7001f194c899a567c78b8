package com.itmskils.apps;

/**
 * access specifier -- public 
 * 
 * 
 */
 class Demo1 {
	
	//definition of m1 
	public  void m1(float f) {
		
		System.out.println(f);
		
	}
	
	public static void main(String [] a) {
		
		Demo1 d1 = new Demo1();
		
		//executing m1 with a fixed value
		d1.m1(5.6f);
		
		d1.m1(9.0f);
		
		
	}
	

}
